  "bugs": {
    "url": "https://github.com/wenjieitk/microservices-booking_payment/issues"
  },
  "homepage": "https://github.com/wenjieitk/microservices-booking_payment#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "concurrently": "^5.2.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "lodash": "^4.17.19",
    "loglevel": "^1.6.8",
    "lowdb": "^1.0.0",
    "uuid": "^8.3.0"
